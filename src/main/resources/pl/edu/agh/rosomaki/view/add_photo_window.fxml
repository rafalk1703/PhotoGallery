<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.rosomaki.controller.AddPhotoController">
    <children>
        <Label layoutX="119.0" layoutY="37.0" text="Photo path" />
        <TextField fx:id="photoPath" layoutX="228.0" layoutY="34.0" />
        <Label layoutX="135.0" layoutY="97.0" text="Name" />
        <TextField fx:id="name" layoutX="228.0" layoutY="94.0" />
        <Label layoutX="135.0" layoutY="157.0" text="Tags" />
        <TextField fx:id="tags" layoutX="228.0" layoutY="154.0" />
        <Label layoutX="122.0" layoutY="214.0" text="Location" />
        <TextField fx:id="where" layoutX="228.0" layoutY="214.0" />
        <Label layoutX="135.0" layoutY="274.0" text="Date" />
        <DatePicker fx:id="datePicker" layoutX="228.0" layoutY="274.0" />
        <Button fx:id="addButton" layoutX="136.0" layoutY="334.0" mnemonicParsing="false" onAction="#addButtonAction" text="Add" />
        <Button fx:id="cancelButton" layoutX="366.0" layoutY="334.0" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" />
    </children>
</AnchorPane>
