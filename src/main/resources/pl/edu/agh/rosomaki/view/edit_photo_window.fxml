<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.rosomaki.controller.EditPhotoController">
    <children>
        <Label layoutX="144.0" layoutY="39.0" text="Name" />
        <TextField fx:id="name" layoutX="228.0" layoutY="35.0" />
        <Label layoutX="143.0" layoutY="98.0" text="Tags" />
        <TextField fx:id="tags" layoutX="228.0" layoutY="94.0" />
        <Label layoutX="125.0" layoutY="168.0" text="Location" />
        <TextField fx:id="where" layoutX="228.0" layoutY="164.0" />
        <Label layoutX="135.0" layoutY="236.0" text="Date" />
        <DatePicker fx:id="datePicker" layoutX="228.0" layoutY="232.0" />
        <Button fx:id="confirmButton" layoutX="136.0" layoutY="334.0" mnemonicParsing="false" onAction="#confirmButtonAction" text="Confirm" />
        <Button fx:id="cancelButton" layoutX="366.0" layoutY="334.0" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" />
    </children>
</AnchorPane>
